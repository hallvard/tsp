{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-tsp-editor"
      ],
      "outFiles": [
        "${workspaceFolder}/vscode-tsp-editor/dist/**/*.js"
      ],
      "preLaunchTask": "npm: compile - vscode-tsp-editor"
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "TspServerLauncher",
      "request": "launch",
      "mainClass": "no.hal.tsp.emf.server.TspServerLauncher",
      "projectName": "tsp-emf"
    }
  ]
}